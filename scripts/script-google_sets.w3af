# This scripts tests the googleSets plugin

plugins
output console,textFile
output config textFile
set fileName output-w3af.txt
set verbose True
back
output config console
set verbose False
back

discovery googleSets
back

target
set target http://localhost/w3af/discovery/google_sets/show.php?os=unix, http://localhost/w3af/discovery/google_sets/show.php?os=linux, http://localhost/w3af/discovery/google_sets/red.html, http://localhost/w3af/discovery/google_sets/blue.html
back

start

assert len( kb.kb.getData('urls', 'urlList') ) == 4

exit