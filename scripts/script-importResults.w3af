# demo that imports results from a file

plugins
output console,textFile
output
output config textFile
set fileName output-w3af.txt
set verbosity 10
back
output config console
set verbosity 0
back

discovery importResults
discovery config importResults
set inputFile scripts/input-test.csv
back

back

target
set target http://localhost/w3af/xss/test-xss.php
back

start

assert len(kb.kb.getData('urls','urlList')) == 4

exit
