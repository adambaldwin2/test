# This is the basic Auth Bruteforce demo

misc-settings
set maxThreads 10
back

plugins
audit htaccessMethods
bruteforce basicAuthBrute
discovery allowedMethods,webSpider
grep httpAuthDetect,passwordProfiling
output console,textFile
output
output config textFile
set fileName output-w3af.txt
set verbose True
back
output config console
set verbose False
back
back
target
set target http://localhost/w3af/bruteforce/basic_auth/
back
start
exit
